{
	"format_version": "1.8.0",
	"animations": {
		"animation.rool": {
			"loop": true,
			"bones": {
				"head": {
					"rotation": {
						"0": "-query.camera_rotation(0)",
						"1": "-query.body_y_rotation+query.camera_rotation(1)+180",
						"2": 0
					},
					"position": {
						"0": 0,
						"1": 0,
						"2": 0
					},
					"scale": "variable.this ? 0.5 : math.clamp(query.distance_from_camera * 0.08, 0.5, 4)"
				}
			}
		}
	}
}